#include <stdio.h>

int main()
{
	int num1,num2;
	scanf("%d",&num1);
	float arr[num1];
	float arr_[1000];
	int plus;
	float equal;
	
	for(int i = 0; i < num1; i++)
	{
		equal = 0;
		scanf("%d",&num2);
		for(int j = 0; j < num2; j++)
		{
			scanf("%d",&plus);
			arr_[j] = plus;
			equal += arr_[j];
		}
		arr[i] = equal/num2;
		equal = 0;
		for(int k = 0; k < num2; k++)
		{
			if(arr[i] < arr_[k])
			{
				equal++;
			}
			else
			{
				continue;
			}
		}
		arr[i] = equal/num2*100;
	}
	
	for(int i = 0; i < num1; i++)
	{
		printf("%.3f%%\n",arr[i]);
	}
	return 0;
}
